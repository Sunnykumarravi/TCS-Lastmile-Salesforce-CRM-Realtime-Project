<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Discount_Percent</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Discount__c &gt;= 0, Discount__c &lt; 100)</errorConditionFormula>
    <errorDisplayField>Discount__c</errorDisplayField>
    <errorMessage>Discount must be between 1% and 100%.</errorMessage>
</ValidationRule>
